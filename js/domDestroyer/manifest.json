{
  "manifest_version": 2,
  "name": "DomDestroyer",
  "description": "Destroy parts of the dom with a click",
  "version": "1",
  "author": "nameofname",
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Notifier",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "index.js"
    ]
  }
}
